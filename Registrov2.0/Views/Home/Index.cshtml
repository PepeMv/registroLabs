@model List<Registrov2._0.Models.ViewModels.LaboratorioVM>

@{
    ViewBag.Title = "Pagina Inicial";
    ViewBag.Laboratorio = "No se encontro Laboratorio";
}

<div class="jumbotron">
    <h1> @ViewBag.Laboratorio</h1>
    @using (Html.BeginForm("", "", ""))
    {
        <div class="row">
            <div class="col-md-12">
                <h2>Ingresa tu cedula:</h2>
                @Html.TextBox("cedula", null, new { @class = "form-control", @type = "number" })
                <p class=""><a href="#" class="btn btn-primary btn-lg p-4 para-padding">Registrar Asistencia &raquo;</a></p>
            </div>
        </div>}

</div>

